<script>
    function changeTab(title) {
        $('#title-content')[0].innerHTML = title;
        var url = "";
        if (title == "Trekking"){
            url = "@Html.Raw(Url.Action("GetTrekkingForm", "Home"))";
        }
        else{
            url = "@Html.Raw(Url.Action("GetComingSoon", "Home"))";
        }
        if (url.length > 0) {
            $('#main-view').load(url, function () {
                enableDatePicker();
            });
        }
    }
</script>